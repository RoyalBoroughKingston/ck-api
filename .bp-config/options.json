{
    "PHP_VERSION": "{PHP_72_LATEST}",
    "PHP_EXTENSIONS": ["openssl", "tokenizer", "mbstring", "pdo", "pdo_mysql"],
    "PHP_MODULES": ["fpm", "cli"],
    "WEBDIR": "public",
    "COMPOSER_VENDOR_DIR": "vendor",
    "COMPOSER_INSTALL_OPTIONS": ["--no-interaction", "--optimize-autoloader", "--no-dev"],
    "ADDITIONAL_PREPROCESS_CMDS": [
        "php artisan migrate --force",
        "php artisan config:cache",
        "php artisan route:cache"
    ]
}
