{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "GNU AGPLv3",
    "type": "project",
    "require": {
        "php": "~7.2.0",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "alphagov/notifications-php-client": "^3.2.0",
        "babenkoivan/scout-elasticsearch-driver": "^4.3.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^2.8",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "firebase/php-jwt": "^5.0",
        "goldspecdigital/oooas": "^2.8",
        "laravel/framework": "^6.0",
        "laravel/passport": "^9.3.2",
        "laravel/scout": "^8.6",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "phpoffice/phpspreadsheet": "^1.4",
        "predis/predis": "^1.1",
        "spatie/laravel-query-builder": "~1.17",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "~1.1",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "pda/pheanstalk": "~3.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": [
            "@test:style",
            "@test:unit"
        ],
        "test:style": [
            "@fix:style --dry-run"
        ],
        "test:unit": [
            "@php vendor/bin/phpunit"
        ],
        "fix:style": [
            "@php vendor/bin/php-cs-fixer fix --config=.php_cs --allow-risky=yes --verbose"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
